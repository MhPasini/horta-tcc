[sub_resource type="GDScript" id="GDScript_1l3pd"]
resource_name = "RemoveArea"
script/source = "extends Panel

func _ready():
	mouse_entered.connect(_on_mouse_entered)
	mouse_exited.connect(_on_mouse_exited)
	hide()

func _can_drop_data(_at_position, data:Variant):
	if data is CodeBlock:
		return true
	else :
		return false

func _drop_data(_at_position, data:Variant):
	if data is CodeBlock:
		var code_container = data.parent_container
		code_container.remove_code_block(data)

func _on_mouse_entered() -> void:
	Input.set_custom_mouse_cursor(load(\"res://UI/Cursors/trash.png\"), Input.CURSOR_CAN_DROP, Vector2(6, 6))

func _on_mouse_exited() -> void:
	Input.set_custom_mouse_cursor(load(\"res://UI/Cursors/ok.png\"), Input.CURSOR_CAN_DROP, Vector2(6, 6))

func _notification(what: int) -> void:
	if what == NOTIFICATION_DRAG_BEGIN and get_viewport().gui_get_drag_data() is CodeBlock:
		show()
	if what == NOTIFICATION_DRAG_END and visible:
		hide()
"

[sub_resource type="GDScript" id="GDScript_d3jd4"]
resource_name = "NewFunc"
script/source = "extends Panel

func _ready():
	mouse_entered.connect(_on_mouse_entered)
	mouse_exited.connect(_on_mouse_exited)
	hide()

func _can_drop_data(_at_position, data:Variant):
	if data is CodeBlock or data is BlockData:
		return true
	else :
		return false

func _drop_data(_at_position, data:Variant):
	if data is CodeBlock or data is BlockData:
		Events.create_new_function.emit(data)

func _on_mouse_entered() -> void:
	Input.set_custom_mouse_cursor(load(\"res://UI/Cursors/save.png\"), Input.CURSOR_CAN_DROP, Vector2(6, 6))

func _on_mouse_exited() -> void:
	Input.set_custom_mouse_cursor(load(\"res://UI/Cursors/ok.png\"), Input.CURSOR_CAN_DROP, Vector2(6, 6))

func _notification(what: int) -> void:
	if what == NOTIFICATION_DRAG_BEGIN and Globals.active_code_tab == 0:
		var data = get_viewport().gui_get_drag_data()
		if data is CodeBlock:
			if data.block_data.type != BlockData.Type.FUNCTION:
				show()
		elif data is BlockData:
			if data.type != BlockData.Type.FUNCTION:
				show()
	if what == NOTIFICATION_DRAG_END and visible:
		hide()
"

