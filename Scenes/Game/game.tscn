[sub_resource type="GDScript" id="GDScript_56h2t"]
resource_name = "Storage"
script/source = "extends Node2D

@onready var carrot_label = $Carrots/CarrotLabel
@onready var onion_label = $Onions/OnionLabel
@onready var radish_label = $Radish/RadishLabel
@onready var potato_label = $Potato/PotatoLabel
@onready var turnip_label = $Turnip/TurnipLabel

var labels : Dictionary = {}

func _ready() -> void:
	labels = {
		\"Cenoura\": carrot_label,
		\"Cebola\": onion_label,
		\"Rabanete\": radish_label,
		\"Batata\": potato_label,
		\"Nabo\": turnip_label,
	}
	Events.update_storage.connect(_on_update_storage)

func _on_update_storage(storage:Dictionary) -> void:
	for element in storage:
		labels[element].text = str(storage[element])

#func _on_icon_hover(icon:Object) -> void:
	#
"

